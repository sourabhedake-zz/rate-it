<div class="col-md-10">
	<div class="row">
		<h1>Enter new category information</h1>

		<%= simple_form_for(@category) do |f| %>

		<div class="form-inputs">
		<%= f.input :category_name %>
		</div>

		<div class="form-actions">
		<%= f.button :submit , :class => "btn-primary"%>
		</div>

		<% end %>
	</div>
</div>